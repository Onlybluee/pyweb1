{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "pyweb",
    "containerDefinitions": [
        {
            "name": "pyweb",
            "image": "314451195721.dkr.ecr.cn-northwest-1.amazonaws.com.cn/cicdtest:latest",
	    "portMappings":[
		    {
			    "containerPort": 1234,
			    "hostPort": 1234,
			    "protocol": "tcp"
		    }
	    ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "3 GB",
    "cpu": "1 vCPU",
    "executionRoleArn": "arn:aws-cn:iam::314451195721:role/ecsTaskExecutionRole"
}
